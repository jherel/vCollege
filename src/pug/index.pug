        // Contact
        section#contact.page-section
            .container
                .text-center
                    h2.section-heading.text-uppercase Contactanos!
                    h3.section-subheading.text-muted Mandanos las dudas que te surjan, o pidenos información si quieres saber como colaborar con nosotros!
                form#contactForm(name='sentMessage', novalidate='novalidate')
                    .row.align-items-stretch.mb-5
                        .col-md-6
                            .form-group
                                input#name.form-control(type='text', placeholder='Your Name *', required='required', data-validation-required-message='Please enter your name.')
